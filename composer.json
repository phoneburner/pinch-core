{
  "name": "phoneburner/pinch-core",
  "description": "Pinch Core: Low-Level Utilities, Helpers and Contracts",
  "license": "MIT",
  "authors": [
    {
      "email": "tim@phoneburner.com",
      "name": "Tim Lytle"
    },
    {
      "email": "andy@phoneburner.com",
      "name": "Andy Snell"
    }
  ],
  "type": "library",
  "require": {
    "php": "^8.4",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-random": "*",
    "ext-sodium": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "nesbot/carbon": "^2.73.0 || ^3.10.1",
    "psr/clock": "^1.0.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "ramsey/uuid": "^4.9.0"
  },
  "require-dev": {
    "phoneburner/composer-replacements": "^1.0.0",
    "phoneburner/pinch-phpstan": "^0.0.30",
    "php-parallel-lint/php-parallel-lint": "^1.4",
    "phpunit/phpunit": "^12.2.7",
    "psr/http-message": "^2.0.0"
  },
  "autoload": {
    "files": [
      "src/functions.php",
      "src/Array/functions.php",
      "src/Attribute/functions.php",
      "src/Enum/functions.php",
      "src/Iterator/functions.php",
      "src/Math/functions.php",
      "src/String/functions.php",
      "src/Time/constants.php",
      "src/Type/functions.php"
    ],
    "psr-4": {
      "PhoneBurner\\Pinch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhoneBurner\\Pinch\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.x-dev"
    },
    "phpstan/extension-installer": {
      "ignore": [
        "phoneburner/pinch-phpstan"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    },
    "platform": {
      "php": "8.4"
    },
    "sort-packages": true
  },
  "provide": {
    "psr/clock-implementation": "1.0"
  }
}
